<UserControl x:Class="Theseus.Code.MVVM.Views.MazeDetailView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Theseus.Code.MVVM.Views"
             xmlns:custom="clr-namespace:Theseus.Code.CustomControls"
             mc:Ignorable="d" 
             d:DesignHeight="784.04" d:DesignWidth="1000">
    <Grid Background="WhiteSmoke">
        <Label Content="Maze View" FontSize="40" HorizontalAlignment="Center" VerticalAlignment="Top" Margin="0 30 0 0" FontFamily="Microsoft YaHei Light"/>

        <Button Style="{StaticResource Generic}" Content="Save" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="800,172,0,0" Command="{Binding SaveMaze}" />
    </Grid>
</UserControl>
